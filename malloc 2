#include <stdio.h>
#include <stdlib.h>

int main() {
    int *arr;
    int size = 5; 

    
    arr = (int *)malloc(size * sizeof(int));
    
    if (arr == NULL) {
        printf("Memory allocation failed!\n");
        return 1;
    }

    
    for (int i = 0; i < size; i++) {
        arr[i] = (i + 1) * 10;
        printf("%d ", arr[i]);
    }
    printf("\n");

    free(arr); 
    return 0;
}
