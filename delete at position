//delete at position
#include <stdio.h>
int main() 
{
    printf("Manasa.Patil\n");
    printf("1SV24CS067\n");
    int a[5] = {10, 20, 30, 40, 50};
    int n = 5;
    int pos = 2;
    for(int i = pos - 1; i < n - 1; i++)
        a[i] = a[i + 1];
    n--;
    for(int i = 0; i < n; i++)
        printf("%d ", a[i]);
    return 0;
}
